{
  "name": "@douglasduteil/framework.docs.cli",
  "description": "Framework docs",
  "license": "WTFPL",
  "private": true,
  "version": "1.0.0",
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-typescript"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@socialgouv/eslint-config-typescript": "^1.57.0",
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^26.0.20",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  },
  "scripts": {
    "precommit": "lint-staged",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint src",
    "lint:fix": "yarn lint --fix",
    "prepush": "yarn lint && yarn test",
    "test": "jest",
    "watch": "yarn build --watch --preserveWatchOutput"
  },
  "sideEffects": false,
  "lint-staged": {
    "src/*.ts": [
      "yarn lint --fix",
      "git add",
      "yarn test --bail --findRelatedTests"
    ]
  },
  "eslint": {
    "extends": "@socialgouv/eslint-config-typescript"
  }
}
